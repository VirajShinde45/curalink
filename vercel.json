{
  "version": 2,
  "buildCommand": "pnpm install --prefer-offline && rm -rf node_modules/.vite-temp && tsc -b && vite build",
  "outputDirectory": "dist",
  "installCommand": "pnpm install --prefer-offline",
  "framework": "vite",
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/assets/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ],
